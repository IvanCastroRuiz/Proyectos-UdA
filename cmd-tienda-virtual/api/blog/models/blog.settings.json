{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "richtext",
      "required": true
    },
    "imagen": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "fechapublic": {
      "type": "date",
      "required": true
    }
  }
}
